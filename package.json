{
  "name": "taro-demo",
  "version": "1.1.0",
  "private": true,
  "isMock": false,
  "description": "taro demo",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "tree": "tree -L 4 -I node_modules",
    "mock": "gulp mock",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/async-await": "1.3.5",
    "@tarojs/cli": "^1.3.5",
    "@tarojs/components": "1.3.5",
    "@tarojs/redux": "1.3.5",
    "@tarojs/redux-h5": "1.3.5",
    "@tarojs/router": "1.3.5",
    "@tarojs/taro": "1.3.5",
    "@tarojs/taro-alipay": "1.3.5",
    "@tarojs/taro-h5": "1.3.5",
    "@tarojs/taro-swan": "1.3.5",
    "@tarojs/taro-tt": "1.3.5",
    "@tarojs/taro-weapp": "1.3.5",
    "nerv-devtools": "^1.4.0",
    "nervjs": "^1.4.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@tarojs/plugin-babel": "1.3.5",
    "@tarojs/plugin-csso": "1.3.5",
    "@tarojs/plugin-less": "1.3.5",
    "@tarojs/plugin-uglifyjs": "1.3.5",
    "@tarojs/webpack-runner": "1.3.5",
    "@types/react": "^16.4.8",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.26.5",
    "classnames": "^2.2.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-taro": "1.3.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-taro": "1.3.5",
    "eslint-plugin-typescript": "^0.12.0",
    "gulp": "3.9.1",
    "gulp-nodemon": "^2.4.2",
    "husky": "^3.0.0",
    "json-server": "^0.15.0",
    "lint-staged": "^8.1.5",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.17.1",
    "standard-version": "^8.0.1",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.5",
    "stylelint-taro-rn": "1.3.5",
    "typescript": "^3.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
